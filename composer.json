{
	"name": "w-oops/laravel-passport",
	"description": "Laravel's Passport PKCE implementation package",
	"autoload": {
		"psr-4": {
			"Woops\\LaravelPassport\\": "src/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Woops\\LaravelPassport\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Wiru",
			"email": "estuardo.quan@gmail.com"
		}
	],
	"require": {
		"laravel/passport": "^10.4"
	},
	"require-dev": {
		"orchestra/testbench": "^7.5"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Woops\\LaravelPassport\\Providers\\PkceServiceProvider",
				"Woops\\LaravelPassport\\Providers\\AuthServiceProvider",
				"Woops\\LaravelPassport\\Providers\\RouteServiceProvider"
			],
			"aliases": {
				"Pkce": "Woops\\LaravelPassport\\Facades\\Pkce"
			}
		}
	}
}
