{
	"name": "winc-oops/passport-pkce",
	"description": "Laravel's Passport PKCE implementation package",
	"autoload": {
		"psr-4": {
			"WincOops\\PassportPkce\\": "src/",
			"WincOops\\PassportPkce\\Tests\\": "tests/"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"WincOops\\PassportPkce\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "Wiru",
			"email": "estuardo.quan@gmail.com"
		}
	],
	"require": {
		"laravel/passport": "^10.4"
	},
	"require-dev": {
		"orchestra/testbench": "^7.5"
	},
	"extra": {
		"laravel": {
			"providers": [
				"WincOops\\PassportPkce\\Providers\\PassportPkceServiceProvider",
				"WincOops\\PassportPkce\\Providers\\RouteServiceProvider",
				"WincOops\\PassportPkce\\Providers\\AuthServiceProvider"
			],
			"aliases": {
				"PassportPkce": "WincOops\\PassportPkce\\Facades\\PassportPkce"
			}
		}
	}
}
