{
	"name": "proxymurder/laravel-passport-oauth",
	"description": "Laravel's Passport single install pkce implementation.",
	"autoload": {
		"psr-4": {
			"ProxyMurder\\LaravelPassportOAuth\\": "src/"
		},
		"files": [
			"helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"ProxyMurder\\LaravelPassportOAuth\\Tests\\": "tests/"
		}
	},
	"authors": [
		{
			"name": "MurderByProxy",
			"email": "ohmygod@proxymurder.com"
		}
	],
	"require": {
		"laravel/passport": "^10.4"
	},
	"require-dev": {
		"orchestra/testbench": "^7.5"
	},
	"extra": {
		"laravel": {
			"providers": [
				"ProxyMurder\\LaravelPassportOAuth\\Providers\\PkceServiceProvider",
				"ProxyMurder\\LaravelPassportOAuth\\Providers\\AuthServiceProvider",
				"ProxyMurder\\LaravelPassportOAuth\\Providers\\RouteServiceProvider"
			],
			"aliases": {
				"Pkce": "ProxyMurder\\LaravelPassportOAuth\\Facades\\Pkce"
			}
		}
	}
}
